{
  "publisher": "simonhe",
  "name": "common-intellisense",
  "displayName": "common-intellisense",
  "version": "0.1.58",
  "packageManager": "pnpm@8.15.9",
  "description": "common-intellisense",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/vscode-common-intellisense#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/vscode-common-intellisense"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vscode-common-intellisense/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "contributes": {
    "configuration": {
      "title": "Common Intellisense",
      "properties": {
        "common-intellisense.ui": {
          "type": "array",
          "default": [
            "auto"
          ],
          "items": {
            "type": "string",
            "enum": [
              "auto",
              "elementUi2",
              "elementPlus2",
              "antd4",
              "antd5",
              "vant4",
              "naiveUi2",
              "antDesignVue4",
              "antDesignVue3",
              "antDesignVue2",
              "antDesignVue1",
              "varlet2",
              "varlet3",
              "vuetify3",
              "chakraUiReact2",
              "primevue3",
              "quasar2",
              "nextui2",
              "nuxtui2",
              "shadcnVue0",
              "radixVue1",
              "arcoDesign2",
              "arcoDesignVue2",
              "uview1",
              "uview2",
              "@tarojs/components3"
            ]
          },
          "description": "Choose the UI framework. If your selection includes `auto`, the extension will ignore the config and automatically detect the UI framework."
        },
        "common-intellisense.showSlots": {
          "type": "boolean",
          "default": true,
          "description": "Whether to show the slots on template completion."
        },
        "common-intellisense.alias": {
          "type": "object",
          "default": {},
          "description": "Alias for the UI framework. If the UI framework you are using is not in the list, you can add an alias here. like { \"xxx\": \"elementUi2\" }"
        },
        "common-intellisense.exclude": {
          "type": "array",
          "default": [
            "**/dist/**",
            "**/node_modules/**"
          ],
          "description": "The directory you want to be skipped"
        }
      }
    },
    "commands": [
      {
        "command": "common-intellisense.pickUI",
        "title": "Specify the UI library you need to prompt"
      }
    ]
  },
  "scripts": {
    "dev": "tsup src/index.ts --external vscode --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode --minify",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@babel/types": "^7.24.9",
    "@rollup/pluginutils": "^5.1.0",
    "@types/node": "^18.19.41",
    "@types/vscode": "1.77.0",
    "@typescript-eslint/typescript-estree": "^6.21.0",
    "@vscode-use/createwebview": "^0.0.13",
    "@vscode-use/utils": "0.1.11",
    "@vue/compiler-sfc": "3.3.4",
    "bing-translate-api": "^2.10.0",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.3.2",
    "find-up": "^6.3.0",
    "pnpm": "^8.15.9",
    "prettier": "^3.3.3",
    "rimraf": "^4.4.1",
    "svelte": "^4.2.18",
    "translate-google": "^1.5.0",
    "tsup": "^6.7.0",
    "typescript": "^5.5.3",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0"
  }
}
