{
  "name": "VarIndexBar",
  "props": {
    "sticky": {
      "value": "",
      "description": "Whether to enable anchor sticky top",
      "description_zh": "是否开启锚点粘顶",
      "default": "true",
      "type": "boolean"
    },
    "sticky-offset-top": {
      "value": "",
      "description": "Anchor offset top when sticky",
      "description_zh": "粘性时锚点偏移顶部",
      "default": "0",
      "type": "number | string"
    },
    "hide-list": {
      "value": "",
      "description": "Whether to hide anchor list",
      "description_zh": "是否隐藏锚点列表",
      "default": "false",
      "type": "boolean"
    },
    "sticky-css-mode": {
      "value": "",
      "description": "Enable native css sticky mode",
      "description_zh": "启用原生 css 粘性模式",
      "default": "false",
      "type": "boolean"
    },
    "z-index": {
      "value": "",
      "description": "z-index",
      "description_zh": "Z 索引",
      "default": "1",
      "type": "string | number"
    },
    "highlight-color": {
      "value": "",
      "description": "Index character highlight color",
      "description_zh": "索引字符突出显示颜色",
      "default": "-",
      "type": "string"
    },
    "duration": {
      "value": "",
      "description": "Animation duration",
      "description_zh": "动画持续时间",
      "default": "0",
      "type": "string | number"
    }
  },
  "methods": [
    {
      "name": "scrollTo",
      "description": "Scroll to the specified anchor point",
      "params": "`index: number | string, options?: IndexBarScrollToOptions`",
      "description_zh": "滚动到指定锚点",
      "value": ""
    }
  ],
  "typeDetail": {
    "IndexBarScrollToOptions": [
      {
        "name": "event",
        "description": "值为 `false` 时阻止触发 `change` 事件",
        "type": "boolean"
      }
    ]
  },
  "events": [
    {
      "name": "click",
      "description": "Triggered when a character in the index bar is clicked",
      "params": "点击索引栏的字符时触发",
      "description_zh": "点击索引栏的字符时触发",
      "value": ""
    },
    {
      "name": "change",
      "description": "Triggered when the currently highlighted index character changes",
      "params": "当前高亮的索引字符变化时触发",
      "description_zh": "当前高亮的索引字符变化时触发",
      "value": ""
    }
  ],
  "link": "https://varletjs.org/v2/#/en-US/index-bar",
  "link_zh": "https://varletjs.org/v2/#/zh-CN/index-bar"
}
