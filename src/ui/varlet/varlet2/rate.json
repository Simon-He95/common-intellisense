{
  "name": "VarRate",
  "props": {
    ":v-model": {
      "value": "",
      "description": "The current rating",
      "description_zh": "当前额定值",
      "default": "0",
      "type": "number",
      "required": true
    },
    "count": {
      "value": "",
      "description": "Total rating",
      "description_zh": "总评分",
      "default": "5",
      "type": "number | string"
    },
    "color": {
      "value": "",
      "description": "The color of the icon when selected",
      "description_zh": "选择时图标的颜色",
      "default": "-",
      "type": "string"
    },
    "icon": {
      "value": "",
      "description": "The icon style when the entire icon is selected",
      "description_zh": "选择整个图标时的图标样式",
      "default": "star",
      "type": "string"
    },
    "empty-color": {
      "value": "",
      "description": "The color of the icon when unchecked",
      "description_zh": "取消选中时图标的颜色",
      "default": "#bdbdbd",
      "type": "string"
    },
    "empty-icon": {
      "value": "",
      "description": "Style of the icon when unchecked",
      "description_zh": "取消选中时图标的样式",
      "default": "star-outline",
      "type": "string"
    },
    "size": {
      "value": "",
      "description": "Icon size, default unit is px",
      "description_zh": "图标大小，默认单位为px",
      "default": "-",
      "type": "number | string"
    },
    "gap": {
      "value": "",
      "description": "Icon spacing, default unit is px",
      "description_zh": "图标间距，默认单位为px",
      "default": "-",
      "type": "number | string"
    },
    "half": {
      "value": "",
      "description": "Whether half selection is allowed",
      "description_zh": "是否允许半选",
      "default": "false",
      "type": "boolean"
    },
    "half-icon": {
      "value": "",
      "description": "The half-selected icon style only works if half is true",
      "description_zh": "仅当一半为 true 时，半选图标样式才有效",
      "default": "star-half-full",
      "type": "string"
    },
    "namespace": {
      "value": "",
      "description": "Icon namespace",
      "description_zh": "图标命名空间",
      "default": "var-icon",
      "type": "string"
    },
    "empty-icon-namespace": {
      "value": "",
      "description": "Empty icon namespace",
      "description_zh": "空图标命名空间",
      "default": "var-icon",
      "type": "string"
    },
    "half-icon-namespace": {
      "value": "",
      "description": "Half icon namespace",
      "description_zh": "半图标命名空间",
      "default": "var-icon",
      "type": "string"
    },
    "disabled": {
      "value": "",
      "description": "Whether to disable rating",
      "description_zh": "是否禁用评级",
      "default": "false",
      "type": "boolean"
    },
    "disabled-color": {
      "value": "",
      "description": "The color of the icon when disabling grading is only effective when disabled is true and has higher priority than color and empty-color",
      "description_zh": "禁用分级时图标的颜色仅在禁用为 true 且优先级高于 color 和 empty-color 时有效",
      "default": "-",
      "type": "string"
    },
    "readonly": {
      "value": "",
      "description": "Whether it is in readonly status. The rating cannot be modified in readonly status.",
      "description_zh": "是否处于只读状态。在只读状态下无法修改评级。",
      "default": "false",
      "type": "boolean"
    },
    "clearable": {
      "value": "",
      "description": "whether to allow to clear",
      "description_zh": "是否允许清除",
      "default": "false",
      "type": "boolean"
    },
    "ripple": {
      "value": "",
      "description": "Whether to use water ripple",
      "description_zh": "是否使用水波纹",
      "default": "true",
      "type": "boolean"
    },
    "rules": {
      "value": "",
      "description": "The validation rules, returns true to indicate that the validation passed. The remaining values are converted to text as user prompts",
      "description_zh": "验证规则返回 true 以指示验证已通过。其余值将转换为文本作为用户提示",
      "default": "-",
      "type": "Array<(v: number) => any>"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "Triggered when the rating changes",
      "params": "评分改变时触发",
      "description_zh": "评分改变时触发",
      "value": ""
    }
  ],
  "link": "https://varletjs.org/v2/#/en-US/rate",
  "link_zh": "https://varletjs.org/v2/#/zh-CN/rate"
}
