{
  "name": "VarSlider",
  "props": {
    ":v-model": {
      "value": "",
      "description": "The current progress value",
      "default": "0",
      "type": "number | [number, number]",
      "required": true,
      "description_zh": "当前进度值"
    },
    "step": {
      "value": "",
      "description": "Step size, the value must be greater than 0",
      "default": "1",
      "type": "string | number",
      "description_zh": "步长，取值必须大于 0"
    },
    "range": {
      "value": "",
      "description": "Whether to enable the double slider",
      "default": "false",
      "type": "boolean",
      "description_zh": "是否启用双滑块"
    },
    "max": {
      "value": "",
      "description": "maximum",
      "default": "100",
      "type": "string | number",
      "description_zh": "最大值"
    },
    "min": {
      "value": "",
      "description": "minimum",
      "default": "0",
      "type": "string | number",
      "description_zh": "最小值"
    },
    "label-visible": {
      "value": [
        "always",
        "normal",
        "never"
      ],
      "description": "Whether to display a label, optionally 'always normal never'",
      "default": "normal",
      "type": "string",
      "description_zh": "是否显示标签，可选值为 `always normal never`"
    },
    "label-text-color": {
      "value": "",
      "description": "Label text color",
      "default": "#fff",
      "type": "string",
      "description_zh": "标签文字颜色"
    },
    "label-color": {
      "value": "",
      "description": "Label color",
      "default": "-",
      "type": "string",
      "description_zh": "标签颜色"
    },
    "active-color": {
      "value": "",
      "description": "The background color of the active track",
      "default": "-",
      "type": "string",
      "description_zh": "已激活的轨道背景颜色"
    },
    "track-color": {
      "value": "",
      "description": "Track background color",
      "default": "-",
      "type": "string",
      "description_zh": "轨道背景颜色"
    },
    "track-height": {
      "value": "",
      "description": "The height of the track",
      "default": "2",
      "type": "string | number",
      "description_zh": "轨道的高度"
    },
    "thumb-size": {
      "value": "",
      "description": "The size of the slider",
      "default": "12",
      "type": "string | number",
      "description_zh": "滑块的大小"
    },
    "thumb-color": {
      "value": "",
      "description": "The background color of the slider",
      "default": "-",
      "type": "string",
      "description_zh": "滑块的背景颜色"
    },
    "disabled": {
      "value": "",
      "description": "Whether to disable or not",
      "default": "false",
      "type": "boolean",
      "description_zh": "是否禁用"
    },
    "readonly": {
      "value": "",
      "description": "Whether it is read-only or not",
      "default": "false",
      "type": "boolean",
      "description_zh": "是否只读"
    },
    "direction": {
      "value": [
        "horizontal",
        "vertical"
      ],
      "description": "Display direction, optionally 'vertical horizontal'",
      "default": "horizontal",
      "type": "string",
      "description_zh": "显示方向，可选值为 `vertical horizontal`"
    },
    "rules": {
      "value": "",
      "description": "Validation rules",
      "default": "-",
      "type": "array",
      "description_zh": "校验规则"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "Triggered when the value changes",
      "params": "value: 当前进度",
      "description_zh": "值改变时触发",
      "value": ""
    },
    {
      "name": "start",
      "description": "Triggered when you start dragging",
      "params": "_",
      "description_zh": "开始拖动时触发",
      "value": ""
    },
    {
      "name": "end",
      "description": "Triggered when the drag ends",
      "params": "value: 当前进度",
      "description_zh": "结束拖动时触发",
      "value": ""
    }
  ],
  "slots": [
    {
      "name": "button",
      "description": "Custom button",
      "description_zh": "自定义按钮"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/en-US/slider",
  "link_zh": "https://varlet.gitee.io/varlet-ui/#/zh-CN/slider"
}
