{
  "name": "VarLink",
  "props": {
    "type": {
      "description": "Link type, can be set to default primary info success warning danger",
      "description_zh": "链接类型，可设置为默认主信息成功警告危险",
      "default": "default",
      "value": "",
      "type": "string"
    },
    "underline": {
      "description": "Underline, Can be set to always hover none",
      "description_zh": "下划线，可以设置为始终悬停无",
      "default": "always",
      "value": "",
      "type": "string"
    },
    "disabled": {
      "description": "Whether to disable link",
      "description_zh": "是否禁用链接",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "text-color": {
      "description": "Link text color",
      "description_zh": "链接文本颜色",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "text-size": {
      "description": "Link font size",
      "description_zh": "链接字体大小",
      "default": "-",
      "value": "",
      "type": "string | number"
    },
    "href": {
      "description": "Same as native hyperlink's href, Have higher priority than to",
      "description_zh": "与本机超链接的 href 相同，优先级高于",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "rel": {
      "description": "Same as the native rel attribute",
      "description_zh": "与本机 rel 属性相同",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "target": {
      "description": "Same as the native target attribute",
      "description_zh": "与本机目标属性相同",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "to": {
      "description": "router-link to, will render as <router-link />",
      "description_zh": "router-link to，将呈现为 <router-link />",
      "default": "-",
      "value": "",
      "type": "string | object"
    },
    "replace": {
      "description": "router-link replace",
      "description_zh": "路由器链路更换",
      "default": "false",
      "value": "",
      "type": "boolean"
    }
  },
  "link": "https://varletjs.org/#/en-US/link",
  "link_zh": "https://varletjs.org/#/en-US/link",
  "typeDetail": {},
  "events": [
    {
      "name": "click",
      "description": "Triggered when the link is clicked",
      "description_zh": "单击链接时触发",
      "params": "event: Event",
      "value": ""
    }
  ],
  "methods": [],
  "slots": [
    {
      "name": "default",
      "description": "Link content",
      "description_zh": "链接内容"
    }
  ]
}
