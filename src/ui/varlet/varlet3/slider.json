{
  "name": "VarSlider",
  "props": {
    "v-model": {
      "description": "The value of slider",
      "description_zh": "滑块的值",
      "default": "0",
      "value": "",
      "type": "number | [number, number]"
    },
    "step": {
      "description": "Step size. The value must greater than 0",
      "description_zh": "步长。该值必须大于 0",
      "default": "1",
      "value": "",
      "type": "string | number"
    },
    "range": {
      "description": "Whether open the dual thumb mode",
      "description_zh": "是否打开双拇指模式",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "max": {
      "description": "The maximum value the slider can slide to",
      "description_zh": "滑块可以滑动到的最大值",
      "default": "100",
      "value": "",
      "type": "string | number"
    },
    "min": {
      "description": "The minimum value the slider can slide to",
      "description_zh": "滑块可以滑动到的最小值",
      "default": "0",
      "value": "",
      "type": "string | number"
    },
    "label-visible": {
      "description": "Whether to display labels, the optional value is always, normal, never",
      "description_zh": "是否显示标签，可选值为始终、正常、从不",
      "default": "normal",
      "value": "",
      "type": "string"
    },
    "label-text-color": {
      "description": "Color of label text",
      "description_zh": "标签文本的颜色",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "label-color": {
      "description": "Background color of label",
      "description_zh": "标签的背景颜色",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "active-color": {
      "description": "Background color of actived track",
      "description_zh": "活动轨道的背景颜色",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "track-color": {
      "description": "Background color of track",
      "description_zh": "曲目的背景颜色",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "track-height": {
      "description": "Height of track",
      "description_zh": "轨道高度",
      "default": "-",
      "value": "",
      "type": "string | number"
    },
    "thumb-size": {
      "description": "Size of thumb",
      "description_zh": "拇指大小",
      "default": "-",
      "value": "",
      "type": "string | number"
    },
    "thumb-color": {
      "description": "Background color of thumb",
      "description_zh": "拇指的背景颜色",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "disabled": {
      "description": "Whether to disable slider",
      "description_zh": "是否禁用滑块",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "readonly": {
      "description": "Whether to readonly slider",
      "description_zh": "是否只读滑块",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "direction": {
      "description": "Direction of slider, Can be set to vertical horizontal",
      "description_zh": "滑块方向，可设置为垂直水平",
      "default": "horizontal",
      "value": "",
      "type": "string"
    },
    "rules": {
      "description": "Validation rules",
      "description_zh": "验证规则",
      "default": "-",
      "value": "",
      "type": "Array<(v: number | number[]) => any>"
    }
  },
  "link": "https://varletjs.org/#/en-US/slider",
  "link_zh": "https://varletjs.org/#/zh-CN/slider",
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "Emitted after slider changed",
      "description_zh": "滑块更改后发出",
      "params": "value: current value",
      "value": ""
    },
    {
      "name": "start",
      "description": "Emitted when start dragged",
      "description_zh": "开始拖动时发出",
      "params": "-",
      "value": ""
    },
    {
      "name": "end",
      "description": "Emitted when end dragged",
      "description_zh": "拖拽时发出",
      "params": "value: current value",
      "value": ""
    }
  ],
  "methods": [],
  "slots": [
    {
      "name": "button",
      "description": "Custom button",
      "description_zh": "自定义按钮"
    }
  ],
  "suggestions": []
}
