{
  "name": "VarPagination",
  "props": {
    "v-model:current": {
      "description": "Current page number",
      "description_zh": "当前页码",
      "default": "1",
      "value": "",
      "type": "string | number"
    },
    "v-model:size": {
      "description": "Number of data items per page",
      "description_zh": "每页数据项数",
      "default": "10",
      "value": "",
      "type": "string | number"
    },
    "total": {
      "description": "Total number of data items",
      "description_zh": "数据项总数",
      "default": "0",
      "value": "",
      "type": "string | number"
    },
    "simple": {
      "description": "Whether to use simple mode",
      "description_zh": "是否使用简单模式",
      "default": "true",
      "value": "",
      "type": "boolean"
    },
    "elevation": {
      "description": "Elevation level, options true false and level of 0-24, not in simple mode",
      "description_zh": "高程级别，选项 true false 和 0-24 级别，不在简单模式下",
      "default": "true",
      "value": "",
      "type": "string | number | boolean"
    },
    "disabled": {
      "description": "Disable pagination",
      "description_zh": "禁用分页",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "show-size-changer": {
      "description": "Whether to show size select",
      "description_zh": "是否显示尺寸选择",
      "default": "true",
      "value": "",
      "type": "boolean"
    },
    "show-quick-jumper": {
      "description": "Whether you can jump to pages directly",
      "description_zh": "是否可以直接跳转到页面",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "max-pager-count": {
      "description": "Number of buttons displayed between ellipses",
      "description_zh": "椭圆之间显示的按钮数",
      "default": "3",
      "value": "",
      "type": "number"
    },
    "size-option": {
      "description": "Specify the sizeChanger options",
      "description_zh": "指定 sizeChanger 选项",
      "default": "[10, 20, 50, 100]",
      "value": "",
      "type": "number[]"
    },
    "show-total": {
      "description": "Show page item's title",
      "description_zh": "显示页面项目的标题",
      "default": "-",
      "value": "",
      "type": "function(total, range)"
    }
  },
  "link": "https://varletjs.org/#/en-US/pagination",
  "link_zh": "https://varletjs.org/#/zh-CN/pagination",
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "Callback after current or size changes",
      "description_zh": "电流或大小更改后的回调",
      "params": "current: number size: number",
      "value": ""
    }
  ],
  "methods": [],
  "slots": [
    {
      "name": "prev",
      "description": "Content of previous button",
      "description_zh": "上一个按钮的内容"
    },
    {
      "name": "next",
      "description": "Content of next button",
      "description_zh": "下一步按钮的内容"
    }
  ],
  "suggestions": []
}
