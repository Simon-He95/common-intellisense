{
  "name": "VarTimePicker",
  "props": {
    "v-model": {
      "description": "Selected time（ISO 8601 format, HH:mm or HH:mm:ss）",
      "description_zh": "选择时间（ISO 8601格式，HH：mm或HH：mm：ss）",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "format": {
      "description": "Picker type, optional values ampm 24hr",
      "description_zh": "拣选器类型，可选值 ampm 24hr",
      "default": "ampm",
      "value": "",
      "type": "string"
    },
    "color": {
      "description": "Picker color",
      "description_zh": "选取器颜色",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "title-color": {
      "description": "Color of picker title. If not specified it will use the color defined by color prop or the default picker color.",
      "description_zh": "选取器标题的颜色。如果未指定，它将使用由颜色道具定义的颜色或默认选取器颜色。",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "hint": {
      "description": "Picker hint",
      "description_zh": "选取器提示",
      "default": "SELECT TIME",
      "value": "",
      "type": "string"
    },
    "elevation": {
      "description": "Elevation level, options true false and level of 0-24",
      "description_zh": "高程级别，选项 true false 和 0-24 级别",
      "default": "false",
      "value": "",
      "type": "string | number | boolean"
    },
    "min": {
      "description": "Minimum allowed time (ISO 8601 format)",
      "description_zh": "允许的最短时间（ISO 8601 格式）",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "max": {
      "description": "Maximum allowed time (ISO 8601 format)",
      "description_zh": "最大允许时间（ISO 8601 格式）",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "allowed-time": {
      "description": "Restricts which time can be selected",
      "description_zh": "限制可以选择的时间",
      "default": "-",
      "value": "",
      "type": "AllowedTime"
    },
    "readonly": {
      "description": "Readonly",
      "description_zh": "只读",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "use-seconds": {
      "description": "Whether to display seconds",
      "description_zh": "是否显示秒",
      "default": "false",
      "value": "",
      "type": "boolean"
    }
  },
  "link": "https://varletjs.org/#/en-US/time-picker",
  "link_zh": "https://varletjs.org/#/zh-CN/time-picker",
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "Emitted after time changed",
      "description_zh": "时间变化后发出",
      "params": "value: string",
      "value": ""
    }
  ],
  "methods": [],
  "slots": [],
  "suggestions": []
}
