{
  "name": "VarActionSheet",
  "props": {
    "v-model:show": {
      "description": "Whether to display the action sheet",
      "description_zh": "是否显示操作表",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "actions": {
      "description": "Action list",
      "description_zh": "操作列表",
      "default": "[]",
      "value": "",
      "type": "Actions"
    },
    "title": {
      "description": "Action sheet title",
      "description_zh": "操作表标题",
      "default": "Select One",
      "value": "",
      "type": "string"
    },
    "overlay": {
      "description": "Whether to display the overlay",
      "description_zh": "是否显示叠加",
      "default": "true",
      "value": "",
      "type": "boolean"
    },
    "overlay-class": {
      "description": "Custom overlay class",
      "description_zh": "自定义叠加类",
      "default": "-",
      "value": "",
      "type": "string"
    },
    "overlay-style": {
      "description": "Custom overlay style",
      "description_zh": "自定义叠加样式",
      "default": "-",
      "value": "",
      "type": "object"
    },
    "lock-scroll": {
      "description": "Whether to disable scrolling penetration, scrolling the Dialog when disabled will not cause the body to scroll",
      "description_zh": "是否禁用滚动渗透，禁用时滚动对话框不会导致正文滚动",
      "default": "true",
      "value": "",
      "type": "boolean"
    },
    "close-on-click-action": {
      "description": "Whether to close the actions sheet when clicking action",
      "description_zh": "是否在单击操作时关闭操作表",
      "default": "true",
      "value": "",
      "type": "boolean"
    },
    "close-on-click-overlay": {
      "description": "Whether to click the overlay to close the action sheet",
      "description_zh": "是否单击叠加层以关闭操作表",
      "default": "true",
      "value": "",
      "type": "boolean"
    },
    "close-on-key-escape": {
      "description": "Whether to support keyboard ESC to close the action sheet",
      "description_zh": "是否支持键盘ESC关闭操作表",
      "default": "true",
      "value": "",
      "type": "boolean"
    },
    "safe-area": {
      "description": "Whether to enable bottom safety zone adaptation",
      "description_zh": "是否启用底部安全区适配",
      "default": "false",
      "value": "",
      "type": "boolean"
    },
    "teleport": {
      "description": "The location of the action sheet to mount",
      "description_zh": "要装载的操作表的位置",
      "default": "body",
      "value": "",
      "type": "TeleportProps['to'] | false"
    }
  },
  "link": "https://varletjs.org/#/en-US/action-sheet",
  "link_zh": "https://varletjs.org/#/zh-CN/action-sheet",
  "typeDetail": {},
