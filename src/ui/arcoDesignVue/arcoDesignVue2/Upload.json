    {
      "name": "upload-button",
      "description": "上传按钮",
      "description_zh": "上传按钮",
      "params": "-"
    },
    {
      "name": "upload-item",
      "description": "上传列表的项目",
      "description_zh": "上传列表的项目",
      "params": "fileItem: FileItemindex: number"
    }
  ]
}
