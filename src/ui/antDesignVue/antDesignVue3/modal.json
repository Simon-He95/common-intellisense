{
  "name": "AModal",
  "props": {
    ":afterClose": {
      "value": "",
      "description": "Callbacks after Modal is completely closed",
      "default": "无",
      "type": "function",
      "description_zh": "Modal 完全关闭后的回调"
    },
    ":bodyStyle": {
      "value": "",
      "description": "Modal body style",
      "default": "{}",
      "type": "object",
      "description_zh": "Modal body 样式"
    },
    ":cancelButtonProps": {
      "value": "",
      "description": "cancel button props",
      "default": "-",
      "type": "[ButtonProps]( | components | button | #api)",
      "description_zh": "cancel 按钮 props"
    },
    "cancelText": {
      "value": "",
      "description": "Cancel button text",
      "default": "取消",
      "type": "string| slot",
      "description_zh": "取消按钮文字"
    },
    "centered": {
      "value": "",
      "description": "Model is displayed vertically centered",
      "default": "false",
      "type": "boolean",
      "description_zh": "垂直居中展示 Modal"
    },
    "closable": {
      "value": "",
      "description": "Whether to display the close button in the upper right corner",
      "default": "true",
      "type": "boolean",
      "description_zh": "是否显示右上角的关闭按钮"
    },
    ":closeIcon": {
      "value": "",
      "description": "Customize the close icon",
      "default": "-",
      "type": "VNode | slot",
      "description_zh": "自定义关闭图标"
    },
    "confirmLoading": {
      "value": "",
      "description": "OK button loading",
      "default": "无",
      "type": "boolean",
      "description_zh": "确定按钮 loading"
    },
    "destroyOnClose": {
      "value": "",
      "description": "Destroys the child elements in the modal when closed",
      "default": "false",
      "type": "boolean",
      "description_zh": "关闭时销毁 Modal 里的子元素"
    },
    "dialogClass": {
      "value": "",
      "description": "Lets you set the class name of the overlay",
      "default": "-",
      "type": "string",
      "description_zh": "可用于设置浮层的类名"
    },
    ":dialogStyle": {
      "value": "",
      "description": "It can be used to set the style of the floating layer, adjust the position of the floating layer, and so on",
      "default": "-",
      "type": "object",
      "description_zh": "可用于设置浮层的样式，调整浮层位置等"
    },
    ":footer": {
      "value": "",
      "description": "Bottom content, when the default bottom button is not needed, can be set to ':footer=\"null\"'",
      "default": "确定取消按钮",
      "type": "string | slot",
      "description_zh": "底部内容，当不需要默认底部按钮时，可以设为 `:footer=\"null\"`"
    },
    "forceRender": {
      "value": "",
      "description": "Force Modal to be rendered",
      "default": "false",
      "type": "boolean",
      "description_zh": "强制渲染 Modal"
    },
    ":getContainer": {
      "value": "",
      "description": "Specifies the HTML node to which the modal mounts",
      "default": "() => document.body",
      "type": "(instance): HTMLElement",
      "description_zh": "指定 Modal 挂载的 HTML 节点"
    },
    "keyboard": {
      "value": "",
      "description": "Whether the keyboard ESC is supported",
      "default": "true",
      "type": "boolean",
      "description_zh": "是否支持键盘 esc 关闭"
    },
    "mask": {
      "value": "",
      "description": "Whether or not to display a mask",
      "default": "true",
      "type": "boolean",
      "description_zh": "是否展示遮罩"
    },
    "maskClosable": {
      "value": "",
      "description": "Click whether the mask is allowed to be turned off",
      "default": "true",
      "type": "boolean",
      "description_zh": "点击蒙层是否允许关闭"
    },
    ":maskStyle": {
      "value": "",
      "description": "Mask style",
      "default": "{}",
      "type": "object",
      "description_zh": "遮罩样式"
    },
    ":okButtonProps": {
      "value": "",
      "description": "OK button props",
      "default": "-",
      "type": "[ButtonProps]( | components | button | #api)",
      "description_zh": "ok 按钮 props"
    },
    "okText": {
      "value": "",
      "description": "Confirm the button text",
      "default": "确定",
      "type": "string | slot",
      "description_zh": "确认按钮文字"
    },
    "okType": {
      "value": "",
      "description": "Confirm the button type",
      "default": "primary",
      "type": "string",
      "description_zh": "确认按钮类型"
    },
    "title": {
      "value": "",
      "description": "title",
      "default": "无",
      "type": "string | slot",
      "description_zh": "标题"
    },
    "v-model:open": {
      "value": "",
      "description": "Whether the dialog box is visible",
      "default": "无",
      "type": "boolean",
      "description_zh": "对话框是否可见",
      "required": true
    },
    "width": {
      "value": "",
      "description": "width",
      "default": "520",
      "type": "string | number",
      "description_zh": "宽度"
    },
    "wrapClassName": {
      "value": "",
      "description": "The class name of the outer container of the dialog box",
      "default": "-",
      "type": "string",
      "description_zh": "对话框外层容器的类名"
    },
    ":zIndex": {
      "value": "",
      "description": "Set the 'z-index' of the modal",
      "default": "1000",
      "type": "number",
      "description_zh": "设置 Modal 的 `z-index`"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "cancel",
      "description": "Click the callback of the mask layer or the top right cross or cancel button",
      "params": "function(e)",
      "description_zh": "点击遮罩层或右上角叉或取消按钮的回调"
    },
    {
      "name": "ok",
      "description": "Click OK callback",
      "params": "function(e)",
      "description_zh": "点击确定回调"
    }
  ],
  "link": "https://antdv.com/components/modal",
  "link_zh": "https://antdv.com/components/modal"
}
