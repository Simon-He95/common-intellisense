{
  "name": "AProgress",
  "props": {
    ":format": {
      "value": "",
      "description": "Template functions for the content",
      "default": "(percent) => percent + `%`",
      "type": "function(percent, successPercent)",
      "description_zh": "内容的模板函数"
    },
    ":percent": {
      "value": "",
      "description": "percentage",
      "default": "0",
      "type": "number",
      "description_zh": "百分比",
      "required": true
    },
    "showInfo": {
      "value": "",
      "description": "Whether to display a progress value or status icon",
      "default": "true",
      "type": "boolean",
      "description_zh": "是否显示进度数值或状态图标"
    },
    "status": {
      "value": "",
      "description": "Status, optional: 'success' 'exception' 'normal' 'active' (line only)",
      "default": "-",
      "type": "string",
      "description_zh": "状态，可选：`success` `exception` `normal` `active`(仅限 line)"
    },
    "strokeColor": {
      "value": "",
      "description": "The color of the circular progress bar, which is a gradient when passed into the object",
      "default": "-",
      "type": "string | object",
      "description_zh": "圆形进度条线的色彩，传入 object 时为渐变"
    },
    "strokeLinecap": {
      "value": [
        "round",
        "butt",
        "square"
      ],
      "description": "The style of the progress bar",
      "default": "round",
      "type": "round / butt / square",
      "description_zh": "进度条的样式"
    },
    ":success": {
      "value": "",
      "description": "Configure the success progress bar",
      "default": "-",
      "type": "{ percent: number, strokeColor: string }",
      "description_zh": "成功进度条相关配置"
    },
    "title": {
      "value": "",
      "description": "html tag title",
      "default": "-",
      "type": "string",
      "description_zh": "html 标签 title"
    },
    "trailColor": {
      "value": "",
      "description": "The color of the unfinished segment",
      "default": "-",
      "type": "string",
      "description_zh": "未完成的分段的颜色"
    },
    "type": {
      "value": [
        "line",
        "circle",
        "dashboard"
      ],
      "description": "Type, optional 'line' or 'circle' dashboard",
      "default": "line",
      "type": "string",
      "description_zh": "类型，可选 `line` `circle` dashboard"
    },
    "size": {
      "value": "",
      "description": "The size of the progress bar",
      "default": "\"default\"",
      "type": "number | \\[number, number] | \"small\" | \"default\"",
      "description_zh": "进度条的尺寸"
    },
    ":steps": {
      "value": "",
      "description": "The total number of steps in the progress bar",
      "default": "-",
      "type": "number",
      "description_zh": "进度条总共步数"
    },
    ":strokeWidth": {
      "value": "",
      "description": "The width of the dashboard progress bar line, in percentage of the width of the progress bar canvas",
      "default": "6",
      "type": "number",
      "description_zh": "仪表盘进度条线的宽度，单位是进度条画布宽度的百分比"
    },
    ":gapDegree": {
      "value": "",
      "description": "The notch angle of the dashboard progress bar can be 0 ~ 295",
      "default": "75",
      "type": "number",
      "description_zh": "仪表盘进度条缺口角度，可取值 0 ~ 295"
    },
    "gapPosition": {
      "value": ["top", "bottom", "left", "right"],
      "description": "Notch position of the dashboard progress bar",
      "default": "bottom",
      "type": "top / bottom / left / right",
      "description_zh": "仪表盘进度条缺口位置"
    }
  },
  "methods": [],
  "events": [],
  "link": "https://antdv.com/components/progress",
  "link_zh": "https://antdv.com/components/progress"
}
