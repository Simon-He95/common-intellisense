{
  "name": "ACarousel",
  "props": {
    "autoplay": {
      "value": "",
      "description": "Whether or not to switch automatically",
      "default": "false",
      "type": "boolean",
      "description_zh": "是否自动切换"
    },
    "dotPosition": {
      "value": [
        "top",
        "bottom",
        "left",
        "right"
      ],
      "description": "The panel indicates the position of the point, and can select 'top', 'bottom', 'left' right",
      "default": "bottom",
      "type": "string",
      "description_zh": "面板指示点位置，可选 `top` `bottom` `left` right"
    },
    "dots": {
      "value": "",
      "description": "Whether to display panel indicator points",
      "default": "true",
      "type": "boolean",
      "description_zh": "是否显示面板指示点"
    },
    "dotsClass": {
      "value": "",
      "description": "The panel indicates the point class name",
      "default": "slick-dots",
      "type": "string",
      "description_zh": "面板指示点类名"
    },
    "easing": {
      "value": "",
      "description": "Animation effects",
      "default": "linear",
      "type": "string",
      "description_zh": "动画效果"
    },
    "effect": {
      "value": ["scrollx", "fade"],
      "description": "Animation effect functions",
      "default": "scrollx",
      "type": "scrollx | fade",
      "description_zh": "动画效果函数"
    },
    ":afterChange": {
      "value": "",
      "description": "Toggle the callback of the panel",
      "default": "-",
      "type": "function(current)",
      "description_zh": "切换面板的回调"
    },
    ":beforeChange": {
      "value": "",
      "description": "Toggle the callback of the panel",
      "default": "-",
      "type": "function(from, to)",
      "description_zh": "切换面板的回调"
    }
  },
  "methods": [
    {
      "name": "goTo",
      "description": "When switching to a specified panel, dontAnimate = true, no animation is used",
      "params": "(slideNumber, dontAnimate)",
      "description_zh": "切换到指定面板, dontAnimate = true 时，不使用动画"
    },
    {
      "name": "next",
      "description": "Switch to the next panel",
      "params": "",
      "description_zh": "切换到下一面板"
    },
    {
      "name": "prev",
      "description": "Switch to the previous panel",
      "params": "",
      "description_zh": "切换到上一面板"
    }
  ],
  "events": [],
  "link": "https://antdv.com/components/carousel",
  "link_zh": "https://antdv.com/components/carousel-cn"
}
