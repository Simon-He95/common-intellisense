{
  "name": "ACollapse",
  "props": {
    "accordion": {
      "value": "",
      "description": "Accordion mode",
      "default": "false",
      "type": "boolean",
      "description_zh": "手风琴模式"
    },
    "v-model:activeKey": {
      "value": "",
      "description": "The key of the currently active tab panel",
      "default": "默认无，accordion 模式下默认第一个元素",
      "type": "string[] | string <br> number[] | number",
      "description_zh": "当前激活 tab 面板的 key",
      "required": true
    },
    "bordered": {
      "value": "",
      "description": "Folded panel with border style",
      "default": "true",
      "type": "boolean",
      "description_zh": "带边框风格的折叠面板"
    },
    "collapsible": {
      "value": [
        "header",
        "icon",
        "disabled"
      ],
      "description": "Whether all sub-panels are collapsible or specify a collapsible trigger area",
      "default": "-",
      "type": "header | icon | disabled",
      "description_zh": "所有子面板是否可折叠或指定可折叠触发区域"
    },
    "destroyInactivePanel": {
      "value": "",
      "description": "Destroy the collapsed hidden panel",
      "default": "false",
      "type": "boolean",
      "description_zh": "销毁折叠隐藏的面板"
    },
    ":expandIcon": {
      "value": "",
      "description": "Customize the toggle icon",
      "default": "",
      "type": "Function(props):VNode | slot=\"expandIcon\" slot-scope=\"props\"|#expandIcon=\"props\"",
      "description_zh": "自定义切换图标"
    },
    "expandIconPosition": {
      "value": [
        "start",
        "end"
      ],
      "description": "Set the icon position",
      "default": "-",
      "type": "start | end",
      "description_zh": "设置图标位置"
    },
    "ghost": {
      "value": "",
      "description": "Make the folded panel transparent and borderless",
      "default": "false",
      "type": "boolean",
      "description_zh": "使折叠面板透明且无边框"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "change",
      "description": "Toggle the callback of the panel",
      "params": "function(key)",
      "description_zh": "切换面板的回调"
    }
  ],
  "suggestions": ["a-collapse-panel"],
  "link": "https://antdv.com/components/collapse",
  "link_zh": "https://antdv.com/components/collapse-cn"
