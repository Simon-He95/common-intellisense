{
  "name": "AQrcode",
  "props": {
    "value": {
      "value": "",
      "description": "The address after the scan",
      "default": "-",
      "type": "string",
      "description_zh": "扫描后的地址",
      "required": true
    },
    "type": {
      "value": [
        "canvas",
        "svg"
      ],
      "description": "Render type",
      "default": "canvas",
      "type": "canvas | svg",
      "description_zh": "渲染类型"
    },
    "icon": {
      "value": "",
      "description": "The address of the image in the QR code (currently only the image address is supported)",
      "default": "-",
      "type": "string",
      "description_zh": "二维码中图片的地址（目前只支持图片地址）"
    },
    ":size": {
      "value": "",
      "description": "QR code size",
      "default": "160",
      "type": "number",
      "description_zh": "二维码大小"
    },
    ":iconSize": {
      "value": "",
      "description": "The size of the image in the QR code",
      "default": "40",
      "type": "number",
      "description_zh": "二维码中图片的大小"
    },
    "color": {
      "value": "",
      "description": "QR code color",
      "default": "`#000`",
      "type": "string",
      "description_zh": "二维码颜色"
    },
    "bgColor": {
      "value": "",
      "description": "QR code background color",
      "default": "transparent",
      "type": "string",
      "description_zh": "二维码背景颜色"
    },
    "bordered": {
      "value": "",
      "description": "Whether there is a border or not",
      "default": "true",
      "type": "boolean",
      "description_zh": "是否有边框"
    },
    "errorLevel": {
      "value": ["L", "M", "Q", "H"],
      "description": "QR code error correction level",
      "default": "M",
      "type": "L / M / Q / H",
      "description_zh": "二维码纠错等级"
    },
    "status": {
      "value": ["active", "expired", "loading"],
      "description": "QR code status",
      "default": "active",
      "type": "active / expired / loading",
      "description_zh": "二维码状态"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "refresh",
      "description": "Click on the \"Click to refresh\" callback",
      "params": "`() => void`",
      "description_zh": "点击\"点击刷新\"的回调"
    }
  ],
  "link": "https://antdv.com/components/qrcode",
  "link_zh": "https://antdv.com/components/qrcode-cn"
}
