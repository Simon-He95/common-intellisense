      "type": "boolean",
      "description_zh": "自动获取焦点"
    },
    ":character": {
      "value": "",
      "description": "Custom characters",
      "default": "`<StarOutlined | >`",
      "type": "string | slot",
      "description_zh": "自定义字符"
    },
    ":count": {
      "value": "",
      "description": "total number of stars",
      "default": "5",
      "type": "number",
      "description_zh": "star 总数"
    },
    "disabled": {
      "value": "",
      "description": "Read-only, unable to interact",
      "default": "false",
      "type": "boolean",
      "description_zh": "只读，无法进行交互"
