      "isRequired": false,
      "description_zh": "分页指示器是否展示"
    },
    "pagination-color": {
      "default": "#fff",
      "value": "",
      "type": "string",
      "description": "分页指示器选中的颜色",
      "isRequired": false,
      "description_zh": "分页指示器选中的颜色"
    },
    "content-close": {
      "default": "true",
