{
  "name": "VanFloatingPanel",
  "props": {
    "v-model:height": {
      "value": "",
      "description": "The display height of the current panel",
      "default": "0",
      "type": "number | string",
      "description_zh": "当前面板的显示高度"
    },
    ":anchors": {
      "value": "",
      "description": "Set a custom anchor point, in px",
      "default": "[100, window.innerWidth * 0.6]",
      "type": "number[]",
      "description_zh": "设置自定义锚点, 单位 px"
    },
    "duration": {
      "value": "",
      "description": "Animation duration, in seconds, is set to 0 to disable animation",
      "default": "0.3",
      "type": "number | string",
      "description_zh": "动画时长，单位秒，设置为 0 可以禁用动画"
    },
    "content-draggable": {
      "value": "",
      "description": "Allows dragging, dragging and dropping of content containers",
      "default": "true",
      "type": "boolean",
      "description_zh": "允许拖拽内容容器"
    },
    "safe-area-inset-bottom": {
      "value": "",
      "description": "Whether to enable [Bottom Security Zone Adaptation](https://vant-docs.netlify.app/#/en-US/advanced-usage#di-bu-an-quan-qu-gua-pei)",
      "default": "true",
      "type": "boolean",
      "description_zh": "是否开启[底部安全区适配](https://vant-docs.netlify.app/#/zh-CN/advanced-usage#di-bu-an-quan-qu-gua-pei)"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "height-change",
      "description": "Triggered when the panel display height changes and the drag ends",
      "params": "{ height: number }",
      "description_zh": "面板显示高度改变且结束拖动后触发",
      "value": ""
    }
  ],
  "slots": [
    {
      "name": "default",
      "description": "Customize panel content",
      "description_zh": "自定义面板内容"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/en-US/floating-panel",
  "link_zh": "https://vant-contrib.gitee.io/vant/#/en-US/floating-panel"
}
